    /* btrc keywords */
    TOK_CLASS, TOK_PUBLIC, TOK_PRIVATE, TOK_SELF, TOK_IN, TOK_PARALLEL,
    TOK_STRING, TOK_BOOL, TOK_TRUE, TOK_FALSE, TOK_NEW, TOK_DELETE,
    TOK_NULL, TOK_TRY, TOK_CATCH, TOK_THROW, TOK_EXTENDS, TOK_VAR,

    /* Built-in types */
    TOK_LIST, TOK_MAP, TOK_ARRAY,

    /* Annotation */
    TOK_AT_GPU,

    /* Operators */
    TOK_PLUS, TOK_MINUS, TOK_STAR, TOK_SLASH, TOK_PERCENT,
    TOK_EQ, TOK_EQ_EQ, TOK_BANG_EQ,
    TOK_LT, TOK_GT, TOK_LT_EQ, TOK_GT_EQ,
    TOK_AMP_AMP, TOK_PIPE_PIPE,
    TOK_BANG, TOK_AMP, TOK_PIPE, TOK_CARET, TOK_TILDE,
    TOK_LT_LT, TOK_GT_GT,
    TOK_PLUS_EQ, TOK_MINUS_EQ, TOK_STAR_EQ, TOK_SLASH_EQ, TOK_PERCENT_EQ,
    TOK_AMP_EQ, TOK_PIPE_EQ, TOK_CARET_EQ, TOK_LT_LT_EQ, TOK_GT_GT_EQ,
    TOK_PLUS_PLUS, TOK_MINUS_MINUS,
    TOK_ARROW, TOK_DOT, TOK_QUESTION, TOK_QUESTION_DOT, TOK_QUESTION_QUESTION,
    TOK_COLON, TOK_COMMA, TOK_SEMICOLON,

    /* Delimiters */
    TOK_LPAREN, TOK_RPAREN, TOK_LBRACKET, TOK_RBRACKET, TOK_LBRACE, TOK_RBRACE,

    /* Special */
    TOK_PREPROCESSOR, TOK_FSTRING_LIT, TOK_EOF
};

