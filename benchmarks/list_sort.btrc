int main() {
    List<int> nums = [];
    int n = 100000;
    /* Simple LCG pseudorandom */
    int seed = 12345;
    for (int i = 0; i < n; i++) {
        seed = (seed * 1103515245 + 12345) % 2147483647;
        int val = seed % n;
        nums.push(val);
    }
    nums.sort();
    printf("sorted %d elements, first=%d last=%d\n", n, nums.get(0), nums.get(n - 1));
    nums.free();
    return 0;
}
