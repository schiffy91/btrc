#include <assert.h>

int main() {
    List<int> a = [1, 2, 3, 4, 5];

    // take
    List<int> t = a.take(3);
    assert(t.size() == 3);
    assert(t.get(0) == 1);
    assert(t.get(2) == 3);

    // drop
    List<int> d = a.drop(2);
    assert(d.size() == 3);
    assert(d.get(0) == 3);
    assert(d.get(2) == 5);

    t.free();
    d.free();
    a.free();

    print("PASS: test_list_take_drop");
    return 0;
}
