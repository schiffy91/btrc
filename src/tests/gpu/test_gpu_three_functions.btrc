#include <assert.h>

@gpu
float[] addKernel(float[] a, float[] b) {
    int i = gpu_id();
    return a[i] + b[i];
}

@gpu
float[] subKernel(float[] a, float[] b) {
    int i = gpu_id();
    return a[i] - b[i];
}

@gpu
float[] mulKernel(float[] a, float[] b) {
    int i = gpu_id();
    return a[i] * b[i];
}

