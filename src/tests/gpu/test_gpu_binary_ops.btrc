#include <assert.h>

@gpu
float[] mathOps(float[] a, float[] b) {
    int i = gpu_id();
    float sum = a[i] + b[i];
    float diff = a[i] - b[i];
    float prod = a[i] * b[i];
    float quot = a[i] / b[i];
    return sum + diff + prod + quot;
}

