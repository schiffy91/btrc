#include <assert.h>

@gpu
void addAndStore(float[] a, float[] b, float[] result) {
    int i = gpu_id();
    result[i] = a[i] + b[i];
    a[i] = 0.0;
    b[i] = 0.0;
}

int main() {
    int a = 5;
    int b = 10;
    int result = a + b;
    assert(result == 15);
    a = 0;
    b = 0;
    assert(a == 0);
    assert(b == 0);
    print("PASS: test_gpu_void_multiple_arrays");
    return 0;
}
