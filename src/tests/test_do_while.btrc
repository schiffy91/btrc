#include <assert.h>

int main() {
    // Basic do-while
    int count = 0;
    do {
        count++;
    } while (count < 5);
    assert(count == 5);

    // Do-while executes at least once even when condition is false
    int x = 10;
    do {
        x += 5;
    } while (x < 5);
    assert(x == 15);

    // Do-while with break
    int sum = 0;
    int i = 0;
    do {
        i++;
        if (i == 3) { break; }
        sum += i;
    } while (i < 10);
    assert(sum == 3);
    assert(i == 3);

    // Do-while with continue
    int total = 0;
    int j = 0;
    do {
        j++;
        if (j % 2 == 0) { continue; }
        total += j;
    } while (j < 6);
    assert(total == 9); // 1 + 3 + 5

    // Nested do-while
    int outer = 0;
    int inner_sum = 0;
    do {
        int k = 0;
        do {
            inner_sum++;
            k++;
        } while (k < 3);
        outer++;
    } while (outer < 2);
    assert(inner_sum == 6);
    assert(outer == 2);

    printf("PASS: test_do_while\n");
    return 0;
}
