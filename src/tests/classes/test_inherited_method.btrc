#include <stdio.h>
class Shape {
    public int sides;
    public Shape(int sides) {
        self.sides = sides;
    }
    public int getSides() {
        return self.sides;
    }
}
class Triangle extends Shape {
    public Triangle() {
        self.sides = 3;
    }
}
int main() {
    Triangle t = Triangle();
    print(t.getSides());
    printf("PASS: test_inherited_method\n");
    return 0;
}
