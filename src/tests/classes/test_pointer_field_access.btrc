#include <stdio.h>
class Point {
    public int x = 0;
    public int y = 0;

    public void set(int nx, int ny) {
        self.x = nx;
        self.y = ny;
    }
}
int main() {
    Point p = new Point();
    p.set(10, 20);
    print(p.x + p.y);
    printf("PASS: test_pointer_field_access\n");
    return 0;
}
