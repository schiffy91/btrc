#include <stdio.h>
#include <math.h>
class Math {
    class int abs(int x) {
        if (x < 0) { return -x; }
        return x;
    }
    class int max(int a, int b) {
        if (a > b) { return a; }
        return b;
    }
    class int min(int a, int b) {
        if (a < b) { return a; }
        return b;
    }
    class float pow(float base, float exp) {
        return powf(base, exp);
    }
    class float sqrt(float x) {
        return sqrtf(x);
    }
}

